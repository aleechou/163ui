					</OnLeave>
				</Scripts>
			</Frame>
			<Button name="$parentSite" parentKey="site">
				<Size x="160" y="22" />
				<Anchors>
					<Anchor point="TOPLEFT" relativeTo="$parentCrest" relativePoint="TOPRIGHT" x="6" y="0" />
				</Anchors>
				<Layers>
					<Layer>
						<FontString name="$parentName" inherits="DigSiteFont" justifyH="LEFT" parentKey="name">
							<Anchors><Anchor point="LEFT" x="0" y="0" /></Anchors>
						</FontString>
					</Layer>
				</Layers>
				<Scripts>
					<OnLoad>
						self:RegisterForClicks("LeftButtonDown", "RightButtonDown")
					</OnLoad>
					<OnClick>
						if (button == "LeftButton") then
							if WorldMapFrame:IsShown() then 
								if self.oldzoneId and (self.oldzoneId == self.zoneId) then
									HideUIPanel(WorldMapFrame)
									self.oldzoneId = nil
								else
									SetMapZoom(GetCurrentMapContinent(), self.zoneId)
									self.oldzoneId = self.zoneId
								end
							else
								ShowUIPanel(WorldMapFrame)
								SetMapZoom(GetCurrentMapContinent(), self.zoneId)
								self.oldzoneId = self.zoneId
							end
						elseif (button == "RightButton") then
							Archy:ToggleSiteBlacklist(self.siteName)
							if Archy:IsSiteBlacklisted(self.siteName) then
								self.name:SetText("|cFFFF0000" .. self.siteName)
							else
								self.name:SetText("|cFFFFFFFF" .. self.siteName)
							end
						end
						Archy:UpdateSiteDistances()
						Archy:RefreshDigSiteDisplay()
					</OnClick>
					<OnEnter>
						Archy:ShowDigSiteTooltip(self)
					</OnEnter>
					<OnLeave>
						GameTooltip:Hide();
					</OnLeave>
				</Scripts>
			</Button>
