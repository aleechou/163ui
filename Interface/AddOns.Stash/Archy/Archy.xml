				</Anchors>
			</Frame>
			<Button name="$parentSurveyButton" parentKey="surveyButton" inherits="ActionButtonTemplate, SecureActionButtonTemplate">
				<Attributes>
					<Attribute name="type" value="spell" />
					<Attribute name="spell" value="80451" />
				</Attributes>
				<Size><AbsDimension x="28" y="24" /></Size>
				<Anchors>
					<Anchor point="LEFT" relativeTo="$parentCircle" relativePoint="RIGHT" x="5" y="0" />
				</Anchors>
				<Frames>
					<Cooldown name="$parentCooldown" inherits="CooldownFrameTemplate" parentKey="cooldown">
						<Size><AbsDimension x="28" y="24"/></Size>
						<Anchors>
							<Anchor point="CENTER">
								<Offset>
									<AbsDimension x="0" y="-1"/>
								</Offset>
							</Anchor>
						</Anchors> 
					</Cooldown>
				</Frames>
				<Scripts>
					<OnLoad>
						self.icon:SetTexture("Interface\\Icons\\inv_misc_shovel_01")
					</OnLoad>
					<OnEnter>
						GameTooltip:SetOwner(self,"ANCHOR_TOP")
						GameTooltip:SetSpellByID(80451)
						GameTooltip:Show()
					</OnEnter>
					<OnLeave>GameTooltip_Hide()</OnLeave>
					<OnHide>GameTooltip_Hide()</OnHide>
				</Scripts>
			</Button>
			<Button name="$parentCrateButton" parentKey="crateButton" inherits="ActionButtonTemplate, SecureActionButtonTemplate">
				<Size><AbsDimension x="28" y="24" /></Size>
				<Anchors>
					<Anchor point="LEFT" relativeTo="$parentSurveyButton" relativePoint="RIGHT" x="10" y="0" />
				</Anchors>
				<Frames>
					<Frame name="$parentShine" parentKey="shine" inherits="AutoCastShineTemplate">
						<Size><AbsDimension x="28" y="24"/></Size>
						<Anchors>
							<Anchor point="CENTER"/>
						</Anchors>
					</Frame>
				</Frames>
				<Scripts>
					<OnLoad>
						if not InCombatLockdown() then self:Disable() end 
						self.icon:SetTexture("Interface\\Icons\\inv_crate_04") 
						self.icon:SetDesaturated(1)
						self:SetMotionScriptsWhileDisabled(true)
						self.tooltip = BROWSE_NO_RESULTS
					</OnLoad>
					<OnEnter>
						if self.tooltip then
							GameTooltip:SetOwner(self,"ANCHOR_TOP")
							if type(self.tooltip) == "string" then
								GameTooltip:SetText(self.tooltip)
							elseif type(self.tooltip) == "number" then
								GameTooltip:SetItemByID(self.tooltip)
							end
							GameTooltip:Show()
						end
					</OnEnter>
					<OnLeave>
						GameTooltip_Hide()
					</OnLeave>
					<OnHide>
						GameTooltip_Hide()
					</OnHide>
				</Scripts>
			</Button>
			<Button name="$parentLorItemButton" parentKey="loritemButton" inherits="ActionButtonTemplate, SecureActionButtonTemplate">
				<Size><AbsDimension x="28" y="24" /></Size>
				<Anchors>
					<Anchor point="LEFT" relativeTo="$parentCrateButton" relativePoint="RIGHT" x="10" y="0" />
				</Anchors>
				<Frames>
					<Cooldown name="$parentCooldown" inherits="CooldownFrameTemplate" parentKey="cooldown">
						<Size><AbsDimension x="28" y="24"/></Size>
						<Anchors>
							<Anchor point="CENTER">
								<Offset>
									<AbsDimension x="0" y="-1"/>
								</Offset>
							</Anchor>
						</Anchors> 
					</Cooldown>
				</Frames>
				<Scripts>
					<OnLoad>
						if not InCombatLockdown() then self:Disable() end 
						self.icon:SetTexture("Interface\\Icons\\inv_misc_map08")
						self.icon:SetDesaturated(1)
						self:SetMotionScriptsWhileDisabled(true)
						self.tooltip = BROWSE_NO_RESULTS
						self:RegisterForClicks("LeftButtonUp", "RightButtonUp")
					</OnLoad>
					<OnEnter>
						if self.tooltip then
							GameTooltip:SetOwner(self,"ANCHOR_TOP")
							if type(self.tooltip) == "string" then
								GameTooltip:SetText(self.tooltip)
							elseif type(self.tooltip) == "number" then
								GameTooltip:SetItemByID(self.tooltip)
							elseif type(self.tooltip) == "table" then
								GameTooltip:SetItemByID(self.tooltip[1])
								GameTooltip:AddLine(" ")
								GameTooltip:AddDoubleLine(_G.HELPFRAME_REPORT_PLAYER_RIGHT_CLICK,self.tooltip[2],1.0,1.0,0, 0,0.8,1.0)
							end
							GameTooltip:Show()
						end
					</OnEnter>
					<OnLeave>GameTooltip_Hide()</OnLeave>
					<OnHide>GameTooltip_Hide()</OnHide>
				</Scripts>
			</Button>			
		</Frames>
		<Scripts>
			<OnLoad>self:RegisterForDrag("LeftButton");</OnLoad>
			<OnDragStart>
				if (Archy.db.profile.general.locked == false and Archy.db.profile.digsite.distanceIndicator.undocked == true) then
					self:StartMoving()
					self.isMoving = true
				end
			</OnDragStart>
			<OnDragStop>
				if (Archy.db.profile.general.locked == false and Archy.db.profile.digsite.distanceIndicator.undocked == true) then
					self:StopMovingOrSizing()
					self.isMoving = false
					if not InCombatLockdown() then
						self:SetUserPlaced(false)
						self:SetFrameLevel(2)
						Archy:SaveFramePosition(self)
					end
				end
			</OnDragStop>
		</Scripts>
	</Frame>
	
	<Frame name="ArchyDigSiteContainer" virtual="true" movable="true" enableMouse="true" clampedToScreen="true">
		<Size>
			<AbsDimension x="340" y="200" />
		</Size>
		<Anchors>
			<Anchor point="CENTER" relativeTo="UIParent" />
		</Anchors>
		<Backdrop bgFile="Interface\TutorialFrame\TutorialFrameBackground" edgeFile="Interface\DialogFrame\UI-DialogBox-Border" tile="true">
			<EdgeSize><AbsValue val="8" /></EdgeSize>
			<TileSize><AbsValue val="8" /></TileSize>
			<BackgroundInsets>
				<AbsInset left="2" right="2" top="2" bottom="2" />
			</BackgroundInsets>
		</Backdrop>
		<Frames>
			<Frame name="$parentContainer" parentKey="container">
				<Anchors>
					<Anchor point="TOPLEFT" relativeTo="$parent" relativePoint="TOPLEFT" x="5" y="-15" />
					<Anchor point="BOTTOMRIGHT" relativeTo="$parent" relativePoint="BOTTOMRIGHT" x="-5" y="5" />
				</Anchors>
			</Frame>
			<CheckButton name="$parentStyleButton" parentKey="styleButton" inherits="UICheckButtonTemplate" frameLevel="2">
				<Size><AbsDimension x="18" y="18" /></Size>
				<Anchors>
					<Anchor point="RIGHT" relativeTo="$parent" relativePoint="TOPRIGHT" x="-5" y="-3" />
				</Anchors>
				<NormalTexture file="Interface\Buttons\UI-PlusButton-Up" />
				<CheckedTexture file="Interface\Buttons\UI-MinusButton-Up" />
				<Scripts>
					<OnLoad>
						self:SetChecked((Archy.db.profile.digsite.style == "Extended"))
						self:SetAlpha(0)
					</OnLoad>
					<OnClick>
						if (self:GetChecked()) then
							Archy.db.profile.digsite.style = "Extended"
						else
							Archy.db.profile.digsite.style = "Compact"
						end
						Archy:UpdateDigSiteFrame()
						Archy:RefreshDigSiteDisplay()
					</OnClick>
					<OnEnter>
						self:SetAlpha(1)
						if self.tooltip then
							GameTooltip:SetOwner(self, "ANCHOR_LEFT");
							GameTooltip:SetText(self.tooltip, nil, nil, nil, nil, 1);
						end
					</OnEnter>
					<OnLeave>
						self:SetAlpha(0)
						GameTooltip:Hide()
					</OnLeave>
				</Scripts>
			</CheckButton>
		</Frames>
		<Scripts>
			<OnLoad>self:RegisterForDrag("LeftButton");</OnLoad>
			<OnDragStart>
				if (Archy.db.profile.general.locked == false) then
					self:StartMoving()
					self.isMoving = true
				end
			</OnDragStart>
			<OnDragStop>
				if (Archy.db.profile.general.locked == false) then
					self:StopMovingOrSizing()
					self.isMoving = false
					self:SetUserPlaced(false)
					self:SetFrameLevel(2)
					
					Archy:SaveFramePosition(self)
				end
			</OnDragStop>
		</Scripts>
	</Frame>
