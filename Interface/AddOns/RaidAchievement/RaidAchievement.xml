	<Size x="33" y="33"/>
	<Anchors>
	<Anchor point="TOPLEFT"/>
	</Anchors>
<Layers>
	<Layer level="OVERLAY">
	<Texture file="Interface\Minimap\MiniMap-TrackingBorder">
	<Size x="56" y="56"/>
	<Anchors>
	<Anchor point="TOPLEFT"/>
	</Anchors>
	</Texture>
	</Layer>
	</Layers>
<Frames>
	<Frame name="RA_MinimapButton_DraggingFrame" hidden="true" frameStrata="FULLSCREEN_DIALOG">
	<Scripts>
	<OnUpdate>
	RA_MinimapButton_DraggingFrame_OnUpdate()
	</OnUpdate>
	</Scripts>
	</Frame>
</Frames>
	<HighlightTexture alphaMode="ADD" file="Interface\Minimap\UI-Minimap-ZoomButton-Highlight"/>
	<Scripts>
	<OnLoad>
	self:RegisterForClicks('LeftButtonUp','RightButtonUp')
	self:RegisterForDrag('LeftButton','RightButton')
	</OnLoad>
	<OnEnter>
	if (not self.dragging) then
	RA_MinimapButton_OnEnter(self)
	end
	</OnEnter>
	<OnLeave>
	GameTooltip:Hide()
	ratpsicon:SetPoint("TOPLEFT",6,-6)
	</OnLeave>
	<OnDragStart>
	ratpsicon:SetPoint("TOPLEFT",6,-6)
	self.dragging = true
	GameTooltip:Hide()
	
	RA_MinimapButton_DraggingFrame:Show()
	</OnDragStart>
	<OnDragStop>
	ratpsicon:SetPoint("TOPLEFT",6,-6)
	self.dragging = false
	
	RA_MinimapButton_DraggingFrame:Hide()
	</OnDragStop>
	<OnClick>
	RA_MinimapButton_OnClick()
	</OnClick>
	<OnMouseDown>
		ratpsicon:SetPoint("TOPLEFT",8,-8)
	</OnMouseDown>
	<OnMouseUp>
		ratpsicon:SetPoint("TOPLEFT",6,-6)
	</OnMouseUp>
	</Scripts>
</Button>




