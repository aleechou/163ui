        <Binding name="DCRSHOW" header="DECURSIVE">
                        DecursiveRootTable.Dcr:HideBar(false);
        </Binding>
        <Binding name="DCRMUFSHOWHIDE">
                        DecursiveRootTable.Dcr:ShowHideDebuffsFrame ();
